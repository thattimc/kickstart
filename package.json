{
  "name": "kickstart",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "concurrently --raw pnpm:format:*",
    "format:prettier": "prettier --ignore-unknown --write .",
    "format:prisma": "prisma format",
    "prepare": "husky install",
    "test": "concurrently --raw pnpm:test:*",
    "test:unit": "vitest --run",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "report": "playwright test --reporter=html",
    "generate:prisma": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.6.1",
    "@types/node": "18.11.11",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "next": "13.0.5",
    "next-auth": "^4.17.0",
    "nodemailer": "^6.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/react": "^13.4.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "vitest": "^0.25.3"
  },
  "prisma": {
    "schema": "src/db/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/db/seed.ts"
  }
}
